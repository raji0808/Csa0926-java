class Main{
    public static void main(String[] args){
        int row,i,j;
        row=5;
        System.out.println(" ");
        for(i=1;i<=row;i++){
            for(int s=1;s<=row-i;s++){
                System.out.print(" ");
                }
            for(j=1;j<=i;j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
